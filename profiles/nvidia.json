{
  "provider": "nvidia",
  "version": "1.0.0",
  "configuration": {
    "apiKey": "",
    "region": "us-west",
    "credentials": {
      "type": "api_key"
    }
  },
  "resources": {
    "compute": {
      "gpuTypes": ["A100", "V100", "T4"],
      "defaultGpuType": "T4"
    },
    "storage": {
      "types": ["NGC Registry", "Object Storage"],
      "defaultType": "NGC Registry"
    },
    "ai": {
      "frameworks": ["TensorFlow", "PyTorch", "RAPIDS"],
      "defaultFramework": "PyTorch"
    }
  },
  "defaults": {
    "tags": {
      "Platform": "AxiomCore",
      "ManagedBy": "UDO"
    }
  },
  "features": {
    "autoScaling": true,
    "monitoring": true,
    "logging": true,
    "gpuOptimization": true
  }
}
