{
  "provider": "local",
  "version": "1.0.0",
  "configuration": {
    "workspacePath": "./workspace",
    "dataPath": "./data",
    "credentials": {
      "type": "none"
    }
  },
  "resources": {
    "compute": {
      "containerRuntime": "docker",
      "maxConcurrency": 4
    },
    "storage": {
      "types": ["Filesystem", "SQLite"],
      "defaultType": "Filesystem"
    },
    "networking": {
      "localhost": true,
      "portRange": "8000-9000"
    }
  },
  "defaults": {
    "tags": {
      "Platform": "AxiomCore",
      "ManagedBy": "UDO",
      "Environment": "Development"
    }
  },
  "features": {
    "autoScaling": false,
    "monitoring": true,
    "logging": true,
    "hotReload": true
  }
}
